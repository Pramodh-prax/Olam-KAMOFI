    <validationRules>
        <fullName>Only_PDI_Member_can_Add_Project_Lead</fullName>
        <active>false</active>
        <errorConditionFormula>isnew() &amp;&amp; NOT(CONTAINS($UserRole.Name, &quot;PD&amp;I&quot;)) &amp;&amp; text(TeamMemberRole) = &quot;PD&amp;I Project Lead&quot; &amp;&amp;
NOT( $Permission.Olam_Validation_Exception )&amp;&amp;
(Opportunity.RecordType.DeveloperName = &quot;Develop_Opportunity&quot; ||
Opportunity.RecordType.DeveloperName = &quot;Defend_Opportunity&quot;)</errorConditionFormula>
        <errorDisplayField>TeamMemberRole</errorDisplayField>
        <errorMessage>Only the PD&amp;I Member can add a PD&amp;I Project Lead</errorMessage>
    </validationRules>
