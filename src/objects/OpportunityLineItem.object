    <validationRules>
        <fullName>Check_if_Project_status_is_Blank</fullName>
        <active>true</active>
        <description>This validation rule will check if the project status is blank before they add the relevant sampling</description>
        <errorConditionFormula>ISNEW() &amp;&amp; 
NOT(ISBLANK(TEXT( Sampling_Status__c ))) &amp;&amp; 
TEXT( Opportunity.Project_Type__c ) = &apos;&apos; &amp;&amp;   
NOT(Opportunity.RecordType.DeveloperName = &apos;GAIN_OPPORTUNITY&apos;||
    Opportunity.RecordType.DeveloperName = &apos;Service&apos; ||
    Opportunity.RecordType.DeveloperName = &apos;Present&apos;) &amp;&amp; 
NOT( $Permission.Olam_Validation_Exception )</errorConditionFormula>
        <errorDisplayField>Sampling_Status__c</errorDisplayField>
        <errorMessage>You must first select the project type before adding the sampling details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dairy_Sales_Project_status_NOT_Sample</fullName>
        <active>true</active>
        <description>This validation rule will ensure the project type is chosen by dairy salesperson with the value only as &quot;Sample&quot;</description>
        <errorConditionFormula>NOT(CONTAINS( TEXT(   Opportunity.Project_Type__c ) , &apos;Sample&apos;)) &amp;&amp;    CONTAINS($UserRole.Name, &apos;Dairy Sales&apos;) &amp;&amp;
(ISCHANGED (  Quantity_of_Sample_Grams__c  ) || ISCHANGED(  Tracking_Number__c   ) || ISCHANGED(   Tracking_URL__c)  || ISCHANGED ( Sampling_Status__c ))</errorConditionFormula>
        <errorMessage>You are not authorised to make this change, Please proceed to submit to PDI</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Followup_date_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
    OR(
         Opportunity.RecordType.DeveloperName = &apos;Defend_Opportunity&apos;,
         Opportunity.RecordType.DeveloperName = &apos;Develop_Opportunity&apos;
     ),
     OR
      (
          ISPICKVAL(Sampling_Status__c , &apos;13&apos;),
          ISPICKVAL(Sampling_Status__c , &apos;14&apos;)
      ) ,
       ISBLANK( Followup_Date__c ) 
     
)</errorConditionFormula>
        <errorDisplayField>Followup_Date__c</errorDisplayField>
        <errorMessage>Please enter the Followup Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NewSampleStatusShouldBeNew</fullName>
        <active>true</active>
        <description>When a new opportunity product is added, the sampling status should be new</description>
        <errorConditionFormula>ISNEW() &amp;&amp;
NOT(ISBLANK(TEXT(Sampling_Status__c))) &amp;&amp;
NOT(ISPICKVAL(Sampling_Status__c,&apos;1&apos;)) &amp;&amp;
ISPICKVAL(Opportunity.Business_Unit__c, &apos;Spice&apos;) &amp;&amp;
(
Opportunity.RecordType.DeveloperName = &quot;Develop_Opportunity&quot; || Opportunity.RecordType.DeveloperName = &quot;Defend_Opportunity&quot;
)</errorConditionFormula>
        <errorDisplayField>Sampling_Status__c</errorDisplayField>
        <errorMessage>Sampling Status should be &apos;New&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPTY011_Sampling_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>(Opportunity.RecordType.DeveloperName = &quot;Develop_Opportunity&quot; ||
Opportunity.RecordType.DeveloperName = &quot;Defend_Opportunity&quot; ) &amp;&amp;

ISPICKVAL(Sampling_Status__c, &quot;5&quot;) &amp;&amp; OR(ISBLANK( Quantity_of_Sample_Grams__c ), Quantity_of_Sample_Grams__c =0)</errorConditionFormula>
        <errorDisplayField>Quantity_of_Sample_Grams__c</errorDisplayField>
        <errorMessage>Please input the Quantity of Sample sent in grams.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPTY011_Sampling_Status_Validation_v2</fullName>
        <active>false</active>
        <errorConditionFormula>( Opportunity.RecordType.DeveloperName = &quot;Develop_Opportunity&quot; ||
 Opportunity.RecordType.DeveloperName  = &quot;Defend_Opportunity&quot; )
&amp;&amp;
ISCHANGED(Sampling_Status__c)
&amp;&amp; 
NOT($Permission.Olam_Validation_Exception)
&amp;&amp;
(ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;Samples Sent&quot;) &amp;&amp; ISPICKVAL(Sampling_Status__c, &quot;New&quot;)) ||

(ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;Pending Feedback&quot;) &amp;&amp; ISPICKVAL(Sampling_Status__c, &quot;Samples Sent&quot;)) ||

(ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;Resampling&quot;) &amp;&amp; NOT(ISPICKVAL(Sampling_Status__c, &quot;Resampling&quot;))) ||

(ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;Sample Rejected&quot;) &amp;&amp; NOT(ISPICKVAL(Sampling_Status__c, &quot;Sample Rejected&quot;))) ||

(ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;Sample Accepted&quot;) &amp;&amp; NOT(ISPICKVAL(Sampling_Status__c , &quot;Sample Accepted&quot;)))</errorConditionFormula>
        <errorDisplayField>Sampling_Status__c</errorDisplayField>
        <errorMessage>Invalid Sample Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OPTY011_Sampling_Tracking_Number_or_URL</fullName>
        <active>false</active>
        <errorConditionFormula>( Opportunity.RecordType.DeveloperName = &quot;Develop_Opportunity&quot; ||
Opportunity.RecordType.DeveloperName = &quot;Defend_Opportunity&quot; ) &amp;&amp;

ISPICKVAL(Sampling_Status__c, &quot;5&quot;) &amp;&amp;
(ISBLANK( Tracking_Number__c )&amp;&amp; ISBLANK( Tracking_URL__c ))</errorConditionFormula>
        <errorDisplayField>Tracking_Number__c</errorDisplayField>
        <errorMessage>Please input the Tracking Number or Tracking URL for the sample</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonForResamplingFieldValQuantity</fullName>
        <active>true</active>
        <description>when Reason for Resampling is &quot;Change in Quantity&quot; -- make &quot;Resampling Quantity&quot; mandatory</description>
        <errorConditionFormula>(
  ISPICKVAL(Reason_for_Resampling__c, &apos;Change in Quantity&apos;) || 
  ISPICKVAL(Reason_for_Resampling__c, &apos;Change in Quantity &amp; Sample Spec&apos;)
) &amp;&amp; ISBLANK(Resampling_Quantity__c)</errorConditionFormula>
        <errorDisplayField>Resampling_Quantity__c</errorDisplayField>
        <errorMessage>Please enter Resampling Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonForResamplingValidation</fullName>
        <active>true</active>
        <errorConditionFormula>(Opportunity.RecordType.DeveloperName = &quot;Develop_Opportunity&quot; ||
Opportunity.RecordType.DeveloperName = &quot;Defend_Opportunity&quot; ) &amp;&amp;

ISPICKVAL(Sampling_Status__c, &quot;10&quot;) &amp;&amp; ISBLANK(TEXT((Reason_for_Resampling__c)))</errorConditionFormula>
        <errorDisplayField>Reason_for_Resampling__c</errorDisplayField>
        <errorMessage>Please select Reason for Resampling</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sample_Feedback_mandatory</fullName>
        <active>true</active>
        <description>Sample Feedback mandatory if sampling status is sample rejected or resampled</description>
        <errorConditionFormula>ISCHANGED(Sampling_Status__c) &amp;&amp; 
OR
 (
   AND(ISPICKVAL(Sampling_Status__c , &apos;10&apos;), IF(ISCHANGED(Sample_Feedback__c), ISBLANK(Sample_Feedback__c), TRUE)),
   AND(ISPICKVAL(Sampling_Status__c , &apos;8&apos;), IF(ISCHANGED(Sample_Feedback__c), ISBLANK(Sample_Feedback__c), TRUE)),
   AND(ISPICKVAL(Sampling_Status__c , &apos;16&apos;), IF(ISCHANGED( Sample_Feedback__c ), ISBLANK(Sample_Feedback__c), TRUE)),
   AND(ISPICKVAL(Sampling_Status__c , &apos;13&apos;), IF(ISCHANGED( Sample_Feedback__c ), ISBLANK(Sample_Feedback__c), TRUE)),
   AND(ISPICKVAL(Sampling_Status__c , &apos;14&apos;), IF(ISCHANGED( Sample_Feedback__c ), ISBLANK(Sample_Feedback__c), TRUE))
 )</errorConditionFormula>
        <errorDisplayField>Sample_Feedback__c</errorDisplayField>
        <errorMessage>Please edit Sample feedback.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SamplingStatusBulkSampleApp</fullName>
        <active>true</active>
        <description>when Reason for Resampling is &quot;Change in Volume&quot; -- make &quot;Resampling Volume&quot; mandatory</description>
        <errorConditionFormula>AND(ISPICKVAL(Sampling_Status__c,&quot;9&quot;), ISBLANK( Bulk_Sample_Quantity__c))</errorConditionFormula>
        <errorDisplayField>Bulk_Sample_Quantity__c</errorDisplayField>
        <errorMessage>This field is mandatory when Sampling Status is &apos;Sample Approved, asking for bulk sample&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SamplingStatusNewOppProdStatusValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(Opportunity.RecordType.DeveloperName = &quot;Develop_Opportunity&quot;,Opportunity.RecordType.DeveloperName = &quot;Defend_Opportunity&quot;) ,
OR(  ISBLANK( Quantity_of_Sample_Grams__c  ), ISBLANK( TEXT(  UoM__c )),  ISBLANK(  TEXT(Sampling_Status__c ))   ))</errorConditionFormula>
        <errorMessage>Please fill Sampling Status, UOM and Sample Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SamplingStatusNewSelectionValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
NOT(ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;1&quot;)),
NOT(ISBLANK(TEXT(Sampling_Status__c))),
ISPICKVAL(Sampling_Status__c,&quot;1&quot;)
) &amp;&amp; not ( $Permission.Olam_Validation_Exception )</errorConditionFormula>
        <errorDisplayField>Sampling_Status__c</errorDisplayField>
        <errorMessage>Field can&apos;t be changed to &apos;New&apos; from the current Sampling Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SamplingStatusOnHoldValidation</fullName>
        <active>false</active>
        <errorConditionFormula>AND
(
OR
(
ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;13&quot;),
ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;14&quot;)),
OR
(
ISPICKVAL(Sampling_Status__c, &quot;1&quot;), ISPICKVAL(Sampling_Status__c, &quot;5&quot;) 
)
)</errorConditionFormula>
        <errorDisplayField>Sampling_Status__c</errorDisplayField>
        <errorMessage>Field can&apos;t be changed to &apos;New&apos; or &apos;Samples Sent&apos; from &apos;Kept on Hold&apos; or &apos;No Response&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SamplingStatusResampleSntToCustValidatn</fullName>
        <active>false</active>
        <errorConditionFormula>AND
(
ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;6&quot;),
ISPICKVAL(Sampling_Status__c,&quot;1&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Sampling_Status__c</errorDisplayField>
        <errorMessage>Field can&apos;t be changed to &apos;New&apos; from &apos;Resample sent to customer&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SamplingStatusSampleSentValidation</fullName>
        <active>false</active>
        <errorConditionFormula>AND
( 
ISPICKVAL(PRIORVALUE(Sampling_Status__c),&quot;5&quot;),
ISPICKVAL(Sampling_Status__c, &quot;1&quot;)
)</errorConditionFormula>
        <errorDisplayField>Sampling_Status__c</errorDisplayField>
        <errorMessage>Field can&apos;t be changed to &apos;New&apos; from &apos;Samples Sent&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Service_Opportunity_Sampling_Status</fullName>
        <active>true</active>
        <errorConditionFormula>Opportunity.RecordType.DeveloperName = &quot;Service&quot; &amp;&amp; 
not( $Permission.Olam_Validation_Exception ) &amp;&amp;
NOT(ISPICKVAL( Sampling_Status__c , &quot;5&quot;))</errorConditionFormula>
        <errorDisplayField>Sampling_Status__c</errorDisplayField>
        <errorMessage>You can only select &quot;Samples Sent&quot; for a Service Opportunity.</errorMessage>
    </validationRules>
</CustomObject>
