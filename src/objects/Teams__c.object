    <validationRules>
        <fullName>Primary_Member_can_be_changed_to_read</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISCHANGED(Access_Level__c) ,  ISPICKVAL(Access_Level__c, &apos;Read Access&apos;) ,  Is_Primary__c )</errorConditionFormula>
        <errorDisplayField>Access_Level__c</errorDisplayField>
