    </validationRules>
    <validationRules>
        <fullName>Restrict_Primary_from_Same_BU</fullName>
        <active>false</active>
        <errorConditionFormula>(ISNEW() &amp;&amp;  CONTAINS( $UserRole.Name , &apos;Dairy&apos;) &amp;&amp;   Is_Primary__c &amp;&amp; NOT((ISBLANK(Account__r.Dairy_Owner_Id__c)))) ||((ISNEW() &amp;&amp;  CONTAINS( $UserRole.Name , &apos;Cocoa&apos;) &amp;&amp;  Is_Primary__c &amp;&amp; NOT((ISBLANK( Account__r.Cocoa_Owner__c ))))) ||
((ISNEW() &amp;&amp;  CONTAINS( $UserRole.Name , &apos;Spice&apos;) &amp;&amp;  Is_Primary__c &amp;&amp; NOT((ISBLANK( Account__r.Spice_Owner_Id__c ))))) ||
((ISNEW() &amp;&amp;  CONTAINS( $UserRole.Name , &apos;Edible_Nuts&apos;) &amp;&amp;  Is_Primary__c &amp;&amp; NOT((ISBLANK( Account__r.Edible_Nuts_Owner_Id__c )))))</errorConditionFormula>
        <errorMessage>Theres an existing Primary Owner assigned for this account</errorMessage>
    </validationRules>
