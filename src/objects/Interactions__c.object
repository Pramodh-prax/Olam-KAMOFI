    <validationRules>
        <fullName>Primary_Acnt_Owner_To_Create_Interaction</fullName>
        <active>false</active>
        <errorConditionFormula>ISNEW() &amp;&amp; NOT( ISBLANK( Account__c ) ) &amp;&amp;
(NOT( $Permission.Olam_Validation_Exception )&amp;&amp;( CASESAFEID($User.Id) &lt;&gt; CASESAFEID( Account__r.Dairy_Owner_Id__c) &amp;&amp; TEXT( Business_Unit__c )= &apos;Dairy&apos;
) || ( CASESAFEID($User.Id) &lt;&gt; CASESAFEID( Account__r.Cocoa_Owner__c ) &amp;&amp; TEXT( Business_Unit__c )= &apos;Cocoa&apos;
))</errorConditionFormula>
        <errorMessage>Only Primary Account Owners can create Interaction</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>User_Is_Not_Account_Team_Member</fullName>
        <active>true</active>
        <errorConditionFormula>isnew() &amp;&amp; User_Is_Account_Team_Member__c = FALSE &amp;&amp; not($Permission.Olam_Validation_Exception)</errorConditionFormula>
        <errorMessage>You cannot create Interactions if you are not part of the Account Team. Add yourself as a Team Member in the Account before creating Interactions.</errorMessage>
    </validationRules>
