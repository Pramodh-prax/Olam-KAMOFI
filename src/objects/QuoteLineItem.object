    <validationRules>
        <fullName>Lock_Record</fullName>
        <active>true</active>
        <description>uote Line Item is not editable if approval status is accepted</description>
        <errorConditionFormula>AND(
ISPICKVAL( PRIORVALUE( Approval_Status__c ) ,&apos;In Progress&apos;),
NOT( ISPICKVAL(Approval_Status__c,&apos;Accepted&apos;)),
NOT( ISPICKVAL(Approval_Status__c,&apos;Rejected&apos;)),
NOT( ISPICKVAL(Approval_Status__c,&apos;Recalled&apos;)),
NOT($Permission.AllowQuoteLineItemEditable),
$Profile.Name != &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>You can not edit the record &apos;In Progress&apos; status</errorMessage>
    </validationRules>
